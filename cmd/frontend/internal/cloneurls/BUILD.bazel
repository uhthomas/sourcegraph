go_test(
    name = "cloneurls_test",
    srcs = ["clone_urls_test.go"],
    embed = [":cloneurls"],
    deps = [
        "//internal/api",
        "//internal/database",
        "//internal/extsvc",
        "//internal/types",
        "@com_github_google_go_cmp//cmp",
    ],
