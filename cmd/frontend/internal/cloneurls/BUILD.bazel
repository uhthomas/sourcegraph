load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "cloneurls",
    srcs = ["clone_urls.go"],
    importpath = "github.com/sourcegraph/sourcegraph/cmd/frontend/internal/cloneurls",
    visibility = ["//cmd/frontend:__subpackages__"],
    deps = [
        "//cmd/frontend/envvar",
        "//internal/api",
        "//internal/conf/reposource",
